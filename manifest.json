  
{
    "manifest_version": 2,
    "name": "covoo",
    "description": "Covoo is a chrome extension that makes all text that says 'coronavirus', turn into a flashy rainbow colour.",
    "icons": {
      "48": "/assets/favicon.png",
      "96": "/assets/favicon.png"
    },
    "version": "0.1.0",
    "content_scripts": [

      {

        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["styles.css"]
      }
    ]
  }